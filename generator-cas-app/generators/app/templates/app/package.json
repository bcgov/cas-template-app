    "normalize.css": "^8.0.1",
    "pg": "^8.7.1",
    "postgraphile": "^4.12.4",
    "postgraphile-log-consola": "^1.0.1",
    "postgraphile-plugin-connection-filter": "^2.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "react-relay": "^12.0.0",
    "react-typography": "^0.16.20",
    "relay-compiler": "^12.0.0",
    "relay-compiler-language-typescript": "^14.2.1",
    "relay-config": "^12.0.0",
    "relay-nextjs": "^0.4.1",
